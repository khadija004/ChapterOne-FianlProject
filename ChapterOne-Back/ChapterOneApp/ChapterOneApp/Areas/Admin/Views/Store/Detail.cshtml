@model Store

@{
    ViewData["Title"] = "Detail";
}

<div class="container my-5">

    <form class="form-sample" asp-action="Create" method="post" enctype="multipart/form-data">


        <div class="form-group">
            <label for="formFile" class="form-label my-2">Location</label>
            <input class="form-control" id="customFile" disabled asp-for="@Model.Location" />
        </div>

        <div class="form-group">
            <label for="formFile" class="form-label my-2">Phone</label>
            <input class="form-control" id="customFile" disabled asp-for="@Model.Phone" />
        </div>

        <div class="form-group">
            <label for="formFile" class="form-label my-2">Mail</label>
            <input class="form-control" id="customFile" disabled asp-for="@Model.Mail" />
        </div>


        <div class="buttons my-3">

            <a class="btn btn-light" asp-action="Index">Cancel</a>
        </div>

    </form>
</div>















